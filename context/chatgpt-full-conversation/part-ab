export interface LoanDetails {
  balance: number;
  apr: number;
  monthlyPayment: number;
}

export interface LOCDetails {
  limit: number;
  apr: number;
  balance: number;
}

export interface SimulationInputs {
  monthlyIncome: number;
  monthlyExpenses: number;
  carLoan: LoanDetails;
  loc?: LOCDetails;
  useVelocity: boolean;
  extraPayment: number;
}

export interface MonthlyResult {
